<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dev.pms.alarm.MessageDAO">
    <insert id="setAlarm" parameterType="AlarmVO">
        INSERT INTO ALARM
        VALUES (NULL,"pizone","1",#{alarmContent},#{alarmDate},"/asdf");
    </insert>

    <select id="getAlarm" resultType="AlarmVO">
        SELECT ALARMCONTENT
        FROM ALARM
    </select>
</mapper>